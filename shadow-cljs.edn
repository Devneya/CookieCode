;; shadow-cljs configuration
{:source-paths
 ["src/main"
  "src/test"]

 :dependencies [[reagent "1.2.0"]
                [org.clojure/tools.cli "1.0.214"]
                [com.taoensso/timbre "6.1.0"]
                [clj-commons/clj-yaml "1.0.26"]
                [failjure/failjure "2.2.0"]
                [cljs-http "0.1.46"]]

 :dev-http
 {8020 "public"}

 :builds
 {:devneya-lib
  {:target :esm
   :output-dir "public/js"
   :entries [devneya.exec devneya.gpt]
   :modules {:api {:exports {makeChain devneya.prompt/make-prompt-chain}}}}
  :devneya
  {:target :browser
   :output-dir "public/js"
   :asset-path "/js"
   :modules
   {:main
    {:init-fn devneya.react/run-react}}}}}