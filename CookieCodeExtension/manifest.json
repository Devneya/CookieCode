{
    "manifest_version": 3,
    "name": "Cookie Code",
    "description": "Code generation tool basen on Devneya library",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["assets/js/cookie-code.js"],
            "css": ["assets/css/cookie-code-style.css"],
            "run_at": "document_end"
        }
    ],
    "host_permissions": ["<all_urls>"]
}